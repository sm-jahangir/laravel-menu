{
  "name": "codersgift/laravel-menu",
  "description": "Drag and drop menu generator like WordPress for Laravel 9+",
  "keywords": [
    "Laravel",
    "Menu",
    "WordPress",
    "Builder",
    "CodersGift"
  ],
  "authors": [
    {
      "name": "Jahangir (Samraat Jahangir)",
      "email": "samraatjahangir@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0 || ^9.0 || ^10 || ^11.0 || ^12.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Vfixtechnology\\Menu\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vfixtechnology\\Menu\\MenuServiceProvider"
      ],
      "aliases": {
        "Menu": "Vfixtechnology\\Menu\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
